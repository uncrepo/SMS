<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.project.sms.Controllers.Admin.AdminController"
            prefHeight="400.0" prefWidth="600.0">
    <TableView fx:id="userTable" layoutX="10" layoutY="10" prefWidth="780" prefHeight="500">
        <columns>
            <TableColumn text="Name" fx:id="nameCol" />
            <TableColumn text="Email" fx:id="emailCol" />
            <TableColumn text="Role" fx:id="roleCol" />
            <TableColumn text="Status" fx:id="statusCol" />
            <TableColumn text="Action" fx:id="actionCol" />
        </columns>
    </TableView>
</AnchorPane>
